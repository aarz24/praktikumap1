#include <iostream>
using namespace std;

int main() {
    double n1, n2, n3;
    cout << "Masukkan tiga angka: ";
    cin >> n1 >> n2 >> n3;

    if(n1 >= n2 && n1 >= n3)
        cout << "Bilangan terbesar: " << n1;
    else if(n2 >= n1 && n2 >= n3)
        cout << "Bilangan terbesar: " << n2;
    else
        cout << "Bilangan terbesar: " << n3;

    return 0;
}
